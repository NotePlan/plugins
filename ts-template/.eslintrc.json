{
    "env": {
        "es6": true,
        "jest": true,
        "node": true
    },
    "extends": [
        "standard",
        "prettier/@typescript-eslint",
        "plugin:prettier/recommended"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly",
        "DataStore": "readonly",
        "Console": "readonly",
        "Editor": "readonly",
        "CommandBar": "readonly",
        "CalendarItem": "readonly",
        "Calendar": "readonly"
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
      "no-unused-vars": 0
    }
}
