{
  "COMMENT": "Details on these fields: https://help.noteplan.co/article/67-create-command-bar-plugins",
  "macOS.minVersion": "10.13.0",
  "noteplan.minAppVersion": "3.4.0",
  "plugin.id": "np.ThemeChooser",
  "plugin.name": "ðŸ§© Theme Chooser",
  "plugin.version": "0.1.0",
  "plugin.description": "Choose from your favorite themes",
  "plugin.author": "dwertheimer",
  "plugin.dependencies": [],
  "plugin.script": "script.js",
  "plugin.url": "https://github.com/NotePlan/plugins/blob/main/np.ThemeChooser/README.md",
  "plugin.commands": [
    {
      "name": "Say Hello from np.ThemeChooser! (change this to your own command)",
      "description": "Your first plugin!",
      "jsFunction": "sayHello",
      "alias": [
        "helloWorld Alias"
      ],
      "arguments": ["Append this text to document"]
    }
  ],
  "plugin.settings": [
    {
      "COMMENT": "Plugin settings documentation: https://help.noteplan.co/article/123-plugin-configuration",
      "type": "heading",
      "title": "Theme Chooser Settings"
    },
    {
      "title": "A string in prefs",
      "key": "settingsString",
      "type": "string",
      "description": "Enter some string and see it change when the plugin is run",
      "default": "This default setting was set in plugin preferences!"
    },
    {
      "key": "version",
      "type": "hidden",
      "description": "Theme Chooser Settings Version"
    }
  ]
}
